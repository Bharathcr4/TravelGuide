<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelguide">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/a_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelGuide"
        android:usesCleartextTraffic="true">
        <activity android:name=".User.Tourist_place_main"></activity>
        <activity android:name=".User.Shopping_main" />
        <activity android:name=".User.Lodging_main" />
        <activity android:name=".User.Hospital_main" />
        <activity android:name=".User.Restaurant_main" />
        <activity android:name=".Common.LoginSignup.Profile_main" />
        <activity android:name=".User.Profile_main" />
        <activity android:name=".User.Car_rent_main" />
        <activity android:name=".User.ProfileActivity_main" />
        <activity android:name=".Common.LoginSignup.ForgotPassword" />
        <activity android:name=".Common.LoginSignup.SignUp" />
        <activity android:name=".Common.LoginSignup.Login" />
        <activity android:name=".Common.LoginSignup.RetailerStartUpScreen" />
        <activity android:name=".User.AllCategories" />
        <activity android:name=".Common.OnBoarding" />
        <activity android:name=".User.UserDashboard" />
        <activity android:name=".Common.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>